  eprint = {1012.2150},
  eprinttype = {arxiv},
  file = {/Users/josephbayley/Zotero/storage/E7IZ32DH/Thrane et al. - 2011 - Long gravitational-wave transients and associated .pdf},
  journal = {Phys. Rev. D},
  keywords = {Astrophysics - Instrumentation and Methods for Astrophysics,General Relativity and Quantum Cosmology},
  language = {en},
  number = {8}
}

@article{tiec2017TheoryGravitational,
