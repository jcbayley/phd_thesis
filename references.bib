  journal = {Phys. Rev. D - Part. Fields, Gravit. Cosmol.},
  number = {8},
  numpages = {20}
}

@article{cutler2002GravitationalWaves,
  title = {Gravitational Waves from Neutron Stars with Large Toroidal \${{B}}\$ Fields},
